<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#10000000"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    tools:context=".Activity.StaffResultStudent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <android.support.v7.widget.CardView
                android:id="@+id/cv_staff_result_student1"
                android:layout_width="match_parent"
                android:contextClickable="false"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:elevation="@dimen/_3sdp"
                android:focusable="true"
                card_view:cardCornerRadius="@dimen/_3sdp">
                <LinearLayout
                    android:id="@+id/text_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="2">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_weight="0.5"
                        android:orientation="vertical">
                        <LinearLayout
                            android:layout_width="@dimen/_225sdp"
                            android:layout_height="@dimen/_40sdp"
                            android:layout_marginRight="@dimen/_80sdp"
                            android:layout_weight="2"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:layout_weight="1.5"
                                android:fontFamily="@font/josefin"
                                android:gravity="center"
                                android:paddingLeft="@dimen/_5sdp"
                                android:text="Name :"
                                android:textSize="@dimen/_14sdp" />
                            <TextView
                                android:id="@+id/staff_result_student_name"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_vertical"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_weight="0.6"
                                android:fontFamily="@font/josefin"
                                android:paddingLeft="@dimen/_5sdp"
                                android:text=""
                                android:textColor="@color/colorPrimary"
                                android:textSize="@dimen/_14sdp" />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="@dimen/_225sdp"
                            android:layout_height="@dimen/_40sdp"
                            android:layout_marginRight="@dimen/_80sdp"
                            android:layout_weight="2"
                            android:orientation="horizontal">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/_5sdp"
                                android:layout_weight="1.5"
                                android:fontFamily="@font/josefin"
                                android:gravity="center"
                                android:text="IC No :"
                                android:textSize="@dimen/_14sdp" />
                            <TextView
                                android:id="@+id/staff_result_student_no"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginEnd="@dimen/_5sdp"
                                android:layout_marginLeft="@dimen/_6sdp"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:layout_weight="0.6"
                                android:fontFamily="@font/josefin"
                                android:maxLines="2"
                                android:paddingLeft="@dimen/_5sdp"
                                android:text=""
                                android:textSize="@dimen/_14sdp" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_weight="1.5">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/staff_result_student_iv"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:paddingLeft="@dimen/_5sdp"
                            android:paddingRight="@dimen/_5sdp"
                            android:src="@drawable/profile"
                            app:civ_border_color="#b1788b"
                            app:civ_border_width="2dp" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:id="@+id/cv_staff_result_student2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cv_staff_result_student1"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:elevation="@dimen/_3sdp"
                android:focusable="true"
                android:foreground="?android:attr/selectableItemBackground"
                card_view:cardCornerRadius="@dimen/_3sdp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:weightSum="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Email :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_email"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:weightSum="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:paddingLeft="@dimen/_5sdp"
                            android:gravity="left"
                            android:text="Mobile :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_mobile"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:weightSum="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Address 1 :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:maxLines="2"
                            android:lines="2"
                            android:gravity="top"
                            android:id="@+id/staff_result_student_address1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:weightSum="2"
                        android:orientation="horizontal">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Address 2 :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:maxLines="2"
                            android:lines="2"
                            android:id="@+id/staff_result_student_address2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <android.support.v7.widget.CardView
                android:contextClickable="false"
                android:id="@+id/cv_staff_result3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cv_staff_result_student2"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginBottom="@dimen/_10sdp"
                android:elevation="@dimen/_3sdp"
                android:focusable="true"
                android:foreground="?android:attr/selectableItemBackground"
                card_view:cardCornerRadius="@dimen/_3sdp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Subject :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_subject"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Mark :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_mark"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:maxLines="2"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Point :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_point"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:maxLines="3"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Grade :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_grade"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:maxLines="3"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_40sdp"
                        android:orientation="horizontal"
                        android:weightSum="2">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:layout_weight="1.3"
                            android:fontFamily="@font/josefin"
                            android:gravity="left"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text="Qualification :"
                            android:textSize="@dimen/_14sdp" />
                        <TextView
                            android:id="@+id/staff_result_student_qualification"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_6sdp"
                            android:layout_marginRight="@dimen/_5sdp"
                            android:layout_marginStart="@dimen/_5sdp"
                            android:layout_weight="0.75"
                            android:fontFamily="@font/josefin"
                            android:maxLines="3"
                            android:paddingLeft="@dimen/_5sdp"
                            android:text=""
                            android:textSize="@dimen/_14sdp" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>